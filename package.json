{
  "name": "react-starter",
  "version": "1.0.0",
  "private": true,
  "description": "React Starter",
  "repository": "exeto/react-starter",
  "author": "Timofey Dergachev <t@exeto.me> (https://exeto.me/en)",
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/dev.js",
    "start": "node buildServer/server.bundle.js",
    "build": "rm -rf buildServer tmp && npm run build:client && npm run build:server",
    "build:client": "node scripts/build.js",
    "build:server": "webpack --progress -p --config config/webpack.config.server.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "**/*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "core-js": "^2.5.5",
    "history": "^4.7.2",
    "koa": "^2.5.1",
    "koa-ejs": "^4.1.1",
    "koa-static": "^4.0.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-jss": "^8.4.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.0",
    "redux-first-router": "^0.0.16-next",
    "redux-first-router-link": "^1.4.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@exeto/eslint-config": "^1.1.0",
    "@exeto/eslint-config-react": "^1.0.0",
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.3.1",
    "eslint": "^4.18.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.10.2",
    "prop-types": "^15.6.0",
    "react-dev-utils": "^5.0.1",
    "react-hot-loader": "^4.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
