    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "postcss": "^8.4.19",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^5.0.3",
    "vite": "^4.0.5"
  },
  "lint-staged": {
    "**/*.{json,yml,css,html,md}": "prettier --write",
